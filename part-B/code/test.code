
public class test {

    public Chromosome CollumnCrosoverTest( final Chromosome other )
    {
        Chromosome child = CollumnCrosover(other);
        child.CalculateScore();
        System.out.println( this.fitness + "   " + other.fitness + "   " + child.fitness );

        for ( int column = 0 ; column < SCHEDULE_LENGHT ; column++ )
        {
            boolean iso_me_mana = true;
            boolean iso_me_patera = true;

            for ( int employee = 0 ; employee < NUM_EMPLOYEES ; employee++ )
                if ( child.genes[employee][column] != this.genes[employee][column] )
                    iso_me_mana = false;

            for ( int employee = 0 ; employee < NUM_EMPLOYEES ; employee++ )
                if ( child.genes[employee][column] != other.genes[employee][column] )
                    iso_me_patera = false;

            if (!iso_me_mana && !iso_me_patera)
                    System.out.print("0 ");
            else if (iso_me_mana && !iso_me_patera)
                System.out.print("1 ");  
            else if (!iso_me_mana && iso_me_patera)
                System.out.print("2 "); 
            else
                System.out.print("4 ");
        }
        return child;
    }
}